@inherits BlazoradeComponentBase
@inject BlazoradeTeamsInteropModule TeamsInterop
@inject NavigationManager NavMan
@inject BlazoradeTeamsOptions AppOptions
@inject BlazoradeLocalizationService LocalizationService
@inject IJSRuntime JSRuntime

@if (this.ShowApplicationTemplate && null != this.ApplicationTemplate)
{
    @this.ApplicationTemplate(this.ApplicationContext)
}
else if(null != this.InitializationException && null != this.InitializationFailedTemplate)
{
    @this.InitializationFailedTemplate(this.InitializationException)
}
else if (null != this.LoadingTemplate)
{
    @this.LoadingTemplate(this.ApplicationContext)
}
